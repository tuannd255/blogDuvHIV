    role="progressbar" aria-valuenow="40" aria-valuemin="0"
    aria-valuemax="100" style="width: <%= percent %>%">
      <span class="sr-only"><%= percent %>% Complete</span>
      <%= title %>
    </div>
  </div>
</div>